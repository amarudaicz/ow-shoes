
<!-- GRID TIENDA -->
<div class="tienda">
    <h3 class="ult-lanzamientos">Ultimos lanzamientos</h3>

    <section class="grid__tienda">

        <% products.forEach(p => { %>
            
            <div class="grid__tienda--producto">
                <img src="/<%=p.thumbnail_image%>">
                <span>"<%= p.title  %> "</span>
                <h3> <%= p.subtitle  %> </h3>

                <div class="block__inputs--tienda">
                    <i class="fa-solid fa-cart-shopping"></i>
                    <a href=<%= `https://ow-shoes.vercel.app/products/get-product?id=${p.id}` %> >
                        <input type="button" value="Comprar" />
                    </a>
                </div>
            </div> 
         
        <% }) %>

    </section>
</div>